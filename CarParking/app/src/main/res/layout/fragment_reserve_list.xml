<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ReserveListFragment"
    android:background="@drawable/yellow">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <TextView
                android:id="@+id/reserveListFragmentTextID"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Reserved Parking Lots"
                android:textSize="35dp"
                android:textColor="#000000"
                android:paddingTop="10dp"
                android:paddingBottom="5dp"
                android:textAlignment="center"
                android:layout_gravity="center"
                android:fontFamily="@font/sfc_artoonist_hand"/>
            <View
                android:layout_width="270dp"
                android:layout_height="1dp"
                android:background="#000000"
                android:layout_gravity="center"
                android:layout_marginBottom="30dp"/>
            <ListView
                android:id="@+id/reserveListViewID"
                android:layout_width="350dp"
                android:layout_height="550dp"
                android:paddingTop="20dp"
                android:layout_gravity="center"
                android:textAlignment="center"/>

            <TextView
                android:id="@+id/outputSampleID"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:textSize="20sp"
                android:textStyle="bold"/>
        </LinearLayout>
    </ScrollView>
</FrameLayout>
